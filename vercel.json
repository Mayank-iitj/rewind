{
  "version": 2,
  "name": "lol-analytics-frontend",
  "public": true,
  "builds": [
    { "src": "frontend/index.html", "use": "@vercel/static" },
    { "src": "frontend/js/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "frontend/index.html" },
    { "src": "/js/(.*)", "dest": "frontend/js/$1" },

    { "src": "/api/(.*)", "dest": "https://YOUR-BACKEND-URL/api/$1" }
  ]
}
